{
	"name": "firestore-store",
	"version": "0.3.3",
	"description": "Firestore session store for Express.js",
	"main": "index.js",
	"keywords": [
		"express",
		"express.js",
		"firestore",
		"express-session",
		"session",
		"session-store",
		"store"
	],
	"scripts": {
		"test": "xo && ava"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hendrysadrak/firestore-store"
	},
	"author": "Hendry Sadrak <hendry.sadrak@outlook.com> (https://www.hendrysadrak.com)",
	"license": "MIT",
	"peerDependencies": {
		"firebase-admin": "^5.x.x"
	},
	"engines": {
		"node": ">=6.0.0"
	},
	"files": [
		"lib/",
		"readme.md",
		"LICENSE",
		"index.js"
	],
	"devDependencies": {
		"ava": "^0.25.0",
		"express-session": "^1.15.6",
		"firebase-admin": "^5.12.1",
		"xo": "^0.21.1"
	},
	"xo": {
		"rules": {
			"space-in-parens": [
				"error",
				"always"
			],
			"object-curly-spacing": [
				"error",
				"always"
			],
			"no-multi-spaces": 0,
			"brace-style": [
				"error",
				"stroustrup"
			],
			"key-spacing": [
				"error",
				{
					"align": "value"
				}
			]
		}
	}
}
